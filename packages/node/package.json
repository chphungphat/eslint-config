    "eslint-plugin-n": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsconfigs": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "!/**/tests",
    "!/**/dist/tests",
    "!/**/__tests__"
  ]
