{
  "name": "@minimaltech/eslint-node",
  "version": "0.2.0",
  "description": "Node ESLint configuration",
  "author": "Minimal Technology <developer@minimaltek.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "release": "pnpm publish --no-git-check",
    "version": "pnpm version --no-git-tag-version"
